            </Min>
            <Max>
                <AbsDimension y="130"/>
            </Max>
        </ResizeBounds>
        <Anchors>
            <Anchor point="TOPLEFT"/>
        </Anchors>
        <Scripts>
            <OnLoad>
                self.Title.TitleString:set_text("Color picker");
                self:register_event("ADDON_LOADED");
                self.currentColor = Color(1,1,1,1);
            </OnLoad>
            <OnEvent>
                if (arg1 == "ADDON_LOADED") then
                    self:set_color(Color(1,1,1,1));
                end
            </OnEvent>
        </Scripts>
        <Frames>
            <Frame name="$parentSwatch">
                <Size>
                    <AbsDimension x="60" y="40"/>
