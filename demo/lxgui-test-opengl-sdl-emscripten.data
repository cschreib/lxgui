                <Anchors>
                    <Anchor point="TOPRIGHT">
                        <Offset>
                            <AbsDimension x="-16" y="32"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Frames>
                    <Frame name="$parentColorZone" setAllPoints="true">
                        <Backdrop edgeFile="|border_2px.png">
                            <BackgroundInsets>
                                <AbsInset left="2" right="2" top="2" bottom="2"/>
                            </BackgroundInsets>
                            <BackgroundColor r="1" g="1" b="1" a="1"/>
                        </Backdrop>
                    </Frame>
                </Frames>
                <Backdrop bgFile="|checker_8px.png" tile="true">
                    <BackgroundInsets>
                        <AbsInset left="2" right="2" top="2" bottom="2"/>
                    </BackgroundInsets>
                </Backdrop>
            </Frame>
            <Button name="$parentCopy" inherits="ButtonTemplate_Button">
                <Anchors>
                    <Anchor point="TOPRIGHT" relativeTo="$parentSwatch" relativePoint="BOTTOMRIGHT">
                        <Offset>
                            <AbsDimension y="5"/>
                        </Offset>
                    </Anchor>
                    <Anchor point="TOPLEFT" relativeTo="$parentSwatch" relativePoint="BOTTOMLEFT">
                        <Offset>
                            <AbsDimension y="5"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnLoad>
                        ColorSelectorCopy:set_text("Copy");
                        self:set_text("Copy");
                    </OnLoad>
                    <OnClick>
                        ColorSelector.savedColor = Color(ColorSelector.currentColor);
                    </OnClick>
                </Scripts>
            </Button>
            <Button name="$parentPaste" inherits="ButtonTemplate_Button">
                <Anchors>
                    <Anchor point="TOPRIGHT" relativeTo="$parentCopy" relativePoint="BOTTOMRIGHT">
                        <Offset>
                            <AbsDimension y="3"/>
                        </Offset>
                    </Anchor>
                    <Anchor point="TOPLEFT" relativeTo="$parentCopy" relativePoint="BOTTOMLEFT">
                        <Offset>
                            <AbsDimension y="3"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnLoad>
                        self:set_text("Paste");
                    </OnLoad>
                    <OnClick>
                        if (ColorSelector.savedColor) then
                            ColorSelector:set_color(ColorSelector.savedColor);
                            ColorSelector:on_color_update();
                        end
                    </OnClick>
                </Scripts>
            </Button>
